tests: True

optional-packages: ./vendored/*/*.cabal

-- avoiding extra dependencies
constraints: rere -rere-cfg
constraints: these -assoc

program-options
  ghc-options: -fno-ignore-asserts

-- import: https://www.stackage.org/lts-21.19/cabal.config
-- import: project-stackage/lts-21.19.local.config
import: project-stackage/lts-21.19.www.config

import: ./project-cabal/pkgs.config
import: ./project-cabal/ghc-9.4.7/constraints.config
build-info: True
